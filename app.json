{
  "expo": {
    "name": "Smart-Finance",
    "slug": "Smart-Finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/Icon.jpeg",
    "scheme": "smartfinance",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Splash.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "SmartFinance.com"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5a92a29e-799f-4e98-9a7a-7a882ec6216e"
      }
    }
  }
}
